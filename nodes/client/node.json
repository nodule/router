{
  "name": "client",
  "ns": "router",
  "description": "Client Side Router, to simplify usage outputs an object keyed by route",
  "async": true,
  "dependencies": {
    "npm": {
      "router-lib": "0.x.x"
    }
  },
  "phrases": {
    "active": "Routing"
  },
  "ports": {
    "input": {
      "routes": {
        "title": "Routes",
        "type": "array",
        "async": true
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "Error"
      },
      "out": {
        "title": "Route",
        "type": "object",
        "additionalProperties": true
      }
    }
  }
}
